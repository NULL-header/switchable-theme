{
  "name": "react-library-init",
  "version": "0.0.1",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc -p tsconfig.build.json",
    "test": "jest",
    "prepare:del": "ls -a -I\".\" -I\"..\" | grep -vE \"dist|package.json|^.git$\" | xargs rm -r | rm dist/empty",
    "prepare:make": "yarn build && yarn prepare:del",
    "prepare:git": "git add . && git commit -m \"release\" && git push",
    "prepare:run": "echo prepare && yarn prepare:make && yarn prepare:git"
  },
  "dependencies": {
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "immutability-helper": "^3.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^7.24.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jest-dom": "^3.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.4.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
